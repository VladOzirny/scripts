						SET !DATASOURCE_LINE 1
						Set счетчикКатегорий {{!COL1}}
						ADD счетчикКатегорий 1
						SET проверкаСчетчика EVAL('var s=\\"{{счетчикКатегорий}}\\"; if(s>\\"4\\"){1} else s;')
