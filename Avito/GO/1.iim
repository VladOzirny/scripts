						CMDLINE !DATASOURCE C:\\Users\\User\\Onedrive\\all\\pc\\1\\категории.txt
						SET !DATASOURCE_COLUMNS 4
						SET !DATASOURCE_LINE {{проверкаСчетчика}}
						set категория1 {{!col1}}
						set категория2 {{!col2}}
						set категория3 {{!col3}}
						set paramId {{!col4}}
